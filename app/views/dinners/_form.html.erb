<%= simple_form_for(@dinner) do |f| %>
  <%= f.error_notification %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :seats, :include_blank => false %>
  <%= f.input :date , :include_blank => false %>
  <%= f.input :image %>
  <%= f.error :base %>
  <%= f.button :submit %>
<% end %>
